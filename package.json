{
  "name": "ams-it",
  "version": "1.0.0",
  "description": "Act Monitoring System for IT - система управления ИТ-активами",
  "main": "server.js",
  "scripts": {
    "start": "node backend/server.js",
    "dev:backend": "nodemon backend/server.js",
    "dev:frontend": "cd frontend && npm start",
    "dev:full": "concurrently \"npm run dev\" \"npm run client\"",
    "dev:log": "node backend/server.js --log",
    "dev:full-log": "concurrently \"npm run dev:log\" \"npm run client\"",
    "install:all": "npm install && cd frontend && npm install",
    "build": "cd frontend && npm run build",
    "test": "jest",
    "lint": "eslint .",
    "dev": "nodemon backend/server.js",
    "client": "cd frontend && npm start"
  },
  "keywords": [
    "it-assets",
    "inventory",
    "management",
    "repair",
    "monitoring"
  ],
  "author": "Кук Бахарев",
  "license": "MIT",
  "dependencies": {
    "antd": "^5.15.0",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.20.2",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.18.2",
    "express-validator": "^7.0.1",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "mysql2": "^3.9.2",
    "nodemailer": "^6.9.5",
    "winston": "^3.10.0"
  },
  "devDependencies": {
    "@types/jest": "^29.5.4",
    "@types/node": "^20.5.7",
    "@typescript-eslint/eslint-plugin": "^6.5.0",
    "@typescript-eslint/parser": "^6.5.0",
    "concurrently": "^8.2.2",
    "eslint": "^8.57.0",
    "jest": "^29.6.4",
    "nodemon": "^3.1.0",
    "prettier": "^3.2.5",
    "ts-jest": "^29.1.1",
    "typescript": "^5.2.2"
  },
  "engines": {
    "node": ">=16.0.0"
  }
}
